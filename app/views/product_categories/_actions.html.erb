<% @product_categories.each do |productcategory| %>
<tr>
    <td><%= productcategory.name %></td>
    <td>
        <%= link_to image_tag("eye-solid.svg", width: 20,float:"right"), product_category_path(productcategory) %>  |  
        <%= link_to image_tag("pen-solid.svg", width: 20,float:"right"), edit_product_category_path(productcategory) %>  |  
        <%= link_to image_tag("trash-solid.svg", width: 20,float:"right"), product_category_path(productcategory), data: { "turbo-method": :delete } %>
    </td>
</tr>
<% end %>