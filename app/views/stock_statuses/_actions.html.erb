<% @stock_statuses.each do |stockstatus| %>
<tr>
    <td><%= stockstatus.status %></td>
    <td>
        <%= link_to image_tag("eye-solid.svg", width: 20,float:"right"), stock_status_path(stockstatus) %>  |  
        <%= link_to image_tag("pen-solid.svg", width: 20,float:"right"), edit_stock_status_path(stockstatus) %>  |  
        <%= link_to image_tag("trash-solid.svg", width: 20,float:"right"), stock_status_path(stockstatus), data: { "turbo-method": :delete } %>
    </td>
</tr>
<% end %>