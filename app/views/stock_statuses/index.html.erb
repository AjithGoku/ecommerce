<div class="container">
  <h1>Stock Status</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Status</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @stock_statuses.each do |stockstatus| %>
      <tr>
        <td><%= stockstatus.status %></td>
        <td>
          <%= link_to 'Show', stock_status_path(stockstatus), class:"btn btn-outline-primary btn-sm" %>
          <%= link_to 'Edit', edit_stock_status_path(stockstatus), class:"btn btn-outline-warning btn-sm" %>
          <%= button_to 'Delete', stock_status_path(stockstatus), method: :delete, class:"btn btn-outline-danger btn-sm" %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <br>
  <%= link_to 'New Stock Status', new_stock_status_path %><br><br>
  <%= link_to 'Back', products_path, class:"btn btn-primary btn-sm" %>
</div>